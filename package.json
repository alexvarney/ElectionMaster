{
  "name": "electionmaster",
  "version": "1.0.0",
  "description": "App for tracking election candidates",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "NODE_ENV=production node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev-remote": "concurrently \"PORT=5001 npm run server\" \"HOST=dev.varney.me DEV_REMOTE=true npm run client\"",
    "dev": "concurrently \"PORT=5001 npm run server\" \"DEV=true npm run client\"",
    "init": "npm install && npm install --prefix client && npm run build --prefix client"
  },
  "author": "Alexander Varney",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.3",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
